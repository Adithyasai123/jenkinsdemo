 

pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building HelloWorld...'
            }
        }
        stage('Test') {
            steps {
                echo 'Running tests...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying HelloWorld...'
            }
        }
    }
}
